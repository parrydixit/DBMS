(D1)
select * from boat where bid in(
  select bid from reserve where sid in(
    select sid from sailor where sname='pranjal'));

bid	bname	bcolor
b01	wave	blue
b02	ocean	red
b04	pond	green


(D2)
select bid, sname
from reserve r join sailor s
on (r.sid==s.sid);

bid	sname
b01	pranjal
b02	pranjal
b02	bhagya
b02	arpit
b04	pranjal
b04	farhan
b05	bhagya


(D3)
select sname, count(bid) from sailor,reserve
where reserve.sid=sailor.sid
group by sname;

sname	count(bid)
arpit	1
bhagya	2
farhan	1
pranjal	3


(D4)
select bname,bid,bcolor,sname
from boat b join sailor s
on (b.bname=s.sname);

bname	bid	bcolor	sname
wave	b01	blue	wave



PL/SQL
set serveroutput on edit prime

DECLARE
  n number:= &n;
  j number:= 0;
  counter number:= 0;
BEGIN  
  while j<=n/2 loop
    if mod(n,j)=0 then
      counter :=1;
      dbms_output.put_line(n||'is not a prime number');
      exit;
    else
      j=j+1;
    end if
  end loop
  
  if counter=0 then 
    dbms_output.put_line(n||'is a prime number');
  end if
END  
/

