create table boat(
    bid varchar2(10),
    bname varchar2(20),
    bcolor varchar2(10),
    primary key(bid));

create table sailor(
    sid varchar2(10),
    sname varchar2(20),
    age number(5),
    rating char(20),
    primary key(sid));

create table reserve(
    bid varchar2(10),
    sid varchar2(10),
    day varchar2(10),
    foreign key(bid) references boat(bid) on delete cascade,
    foreign key(sid) references sailor(sid) on delete cascade,
    primary key(bid,sid));

insert into boat values('&bid','&bname','bcolor');

bid	bname	bcolor
b01	wave	blue
b02	ocean	red
b03	well	white
b04	pond	green
